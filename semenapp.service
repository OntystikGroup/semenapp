[Unit]
Description=Uvicorn instance to serve semenapp
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/root/projects/semenapp
Environment="PATH=/home/root/projects/semenapp/venv/bin"
ExecStart=/home/root/projects/semenapp/venv/bin/uvicorn api.main:app --host 0.0.0.0 --port 8001 --workers 1

[Install]
WantedBy=multi-user.target